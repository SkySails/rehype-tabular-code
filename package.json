  },
  "dependencies": {
    "unist-util-visit": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/unist": "2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "typescript": "4.2.4"
  },
  "prettier": {
    "arrowParens": "avoid",
