{
  "name": "rehype-tabular-code",
  "version": "1.0.0",
  "description": "Rehype plugin for bundling code blocks together under tabs.",
  "license": "MIT",
  "files": [
    "dist/index.d.ts",
    "dist/index.js"
  ],
  "main": "dist/index.js",
  "type": "module",
  "typings": "dist/index.d.ts",
  "scripts": {
